{
    "name": "vue-sample",
    "description": "A Vue.js project",
    "version": "1.0.0",
    "author": "Oahehc <oahehc@gmail.com>",
    "private": true,
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --watch --open --hot",
        "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
        "serveBuild": "cross-env NODE_ENV=production webpack-dev-server --content-base=build --port=9000 --open",
        "selenium-setup": "node ./test/selenium/selenium-setup.js",
        "e2e": "yarn selenium-setup && node ./test/e2e/startup.js",
        "e2e:chrome": "yarn selenium-setup && node ./test/e2e/startup.js --chrome"
    },
    "dependencies": {
        "lodash": "^4.17.4",
        "phantomjs-prebuilt": "^2.1.14",
        "vue": "^2.2.1",
        "vue-router": "^2.3.0",
        "vuex": "^2.2.1"
    },
    "devDependencies": {
        "babel-core": "^6.0.0",
        "babel-loader": "^6.0.0",
        "babel-plugin-transform-async-generator-functions": "^6.22.0",
        "babel-plugin-transform-async-to-generator": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.23.0",
        "babel-preset-latest": "^6.0.0",
        "clean-webpack-plugin": "^0.1.16",
        "cross-env": "^3.0.0",
        "cross-spawn": "^5.1.0",
        "css-loader": "^0.25.0",
        "extract-text-webpack-plugin": "^2.1.0",
        "file-loader": "^0.10.1",
        "html-loader": "^0.4.5",
        "html-webpack-plugin": "^2.28.0",
        "nightwatch": "^0.9.14",
        "node-sass": "^4.5.0",
        "postcss-loader": "^1.3.3",
        "raw-loader": "^0.5.1",
        "sass-loader": "^5.0.1",
        "selenium-standalone": "^6.1.0",
        "style-loader": "^0.14.1",
        "url-loader": "^0.5.8",
        "vue-loader": "^11.1.4",
        "vue-template-compiler": "^2.2.1",
        "webpack": "^2.2.0",
        "webpack-dev-server": "^2.2.0"
    }
}
